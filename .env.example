# Database
DB_HOST=localhost
DB_PORT=5432
DB_USER=orchestrator
DB_PASSWORD=your_password_here
DB_NAME=orchestrator

# JWT
JWT_SECRET=your_super_secret_jwt_key_change_this_in_production

# App
NEXT_PUBLIC_API_URL=http://localhost:3000
NODE_ENV=development

# AWS S3 (for call recording downloads)
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_S3_BUCKET_NAME=your_s3_bucket_name

# Admin API (for orchestrator cache management)
# Base URL of the orchestrator Admin API
ADMIN_API_BASE_URL=http://localhost:8000
# Shared secret key for HMAC-SHA256 request signing
ADMIN_API_KEY=your_admin_api_key_here

# Google OAuth (Better Auth)
# Create credentials at https://console.cloud.google.com/apis/credentials
# Set authorized redirect URI to: {BETTER_AUTH_URL}/api/auth/callback/google
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_google_client_secret

# Better Auth
# Base URL for OAuth callbacks (defaults to NEXT_PUBLIC_API_URL if not set)
BETTER_AUTH_URL=http://localhost:3000
# Secret for signing tokens (defaults to JWT_SECRET if not set)
BETTER_AUTH_SECRET=your_better_auth_secret_change_this_in_production
